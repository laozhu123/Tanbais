<manifest xmlns:android="http://schemas.android.com/apk/res/android"
     package="com.neteaseyx.image">

    <application android:label="@string/app_name"
        android:theme="@style/VsTheme"
        >

        <activity android:name="com.neteaseyx.image.ugallery.activity.ActivityGalleryImage"
            android:screenOrientation="portrait"
            />

        <activity android:name="com.yalantis.ucrop.UCropActivity"
            android:screenOrientation="portrait" />
        <!--android:theme="@style/VsTheme"-->
        <!--/>-->
        <activity android:name="com.neteaseyx.image.ugallery.activity.ActivityTakePhotos" />
        <!--android:theme="@style/VsTheme"-->
        <activity android:name="com.neteaseyx.image.ugallery.activity.ActivityCropImage" />
        <!--android:theme="@style/VsTheme"-->
        <!--android:screenOrientation="portrait"/>-->
        <activity android:name="com.neteaseyx.image.ugallery.activity.ActivityPreviewImage"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.FullScreenTheme"
             />
        <!--android:theme="@style/VsTheme"-->
        <!--android:screenOrientation="portrait"/>-->


    </application>
    <!--在SDCard中创建与删除文件权限-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!--往SDCard写入数据权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--从SDCard读取数据权限-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--网络请求-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
</manifest>
